{
  "expo": {
    "name": "afterplus",
    "slug": "afterplus-mobile",
    "scheme": "afterplus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/after-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/after-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#BC203C"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/after-icon.png",
        "backgroundColor": "#BC203C"
      },
      "package": "com.nauravalda.afterplusmobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f8e0b8c4-0de3-49d6-92fd-663a09f0bfb9"
      }
    },
    "owner": "nauravalda"
  }
}
